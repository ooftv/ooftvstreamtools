<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>GroupName</key>
	<string>staff</string>
	<key>InitGroups</key>
	<true/>
	<key>Label</key>
	<string>tv.outoffocus.restartstream</string>
	<key>ProgramArguments</key>
	<array>
		<string>/Applications/VLC.app/Contents/MacOS/VLC</string>
		<string>--fullscreen</string>
		<string>--loop</string>
		<string>/Users/ooftv/Movies/ooftv/playlists/today.m3u</string>
	</array>
	<key>RunAtLoad</key>
	<true/>
	<key>StandardErrorPath</key>
	<string>/tmp/tv.outoffocus.restartstream.stderr</string>
	<key>StandardOutPath</key>
	<string>/tmp/tv.outoffocus.restartstream.stdout</string>
	<key>StartCalendarInterval</key>
	<dict>
		<key>Hour</key>
		<integer>4</integer>
		<key>Minute</key>
		<integer>52</integer>
	</dict>
	<key>UserName</key>
	<string>ooftv</string>
</dict>
</plist>
